    color: #333333;
    vertical-align: baseline;
  }

  blockquote, q {
    quotes: none;
  }

  blockquote:before, blockquote:after,
  q:before, q:after {
    content: '';
    content: none;
  }

  b, strong {
    font-weight: bold;
  }

  i, em {
    font-style: italic;
  }

  strong, sup, sub, em, s, u {
    color: inherit;
  }

</style>

