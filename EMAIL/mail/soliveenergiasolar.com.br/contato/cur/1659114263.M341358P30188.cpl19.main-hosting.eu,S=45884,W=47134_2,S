  h1, h2, h3, h4, h5, h6 {
    font-weight: 500;
  }

  ol, ul {
    font-size: 100%;
    font: inherit;
    color: #333333;
    vertical-align: baseline;
  }

  blockquote, q {
    quotes: none;
  }

  blockquote:before, blockquote:after,
  q:before, q:after {
    content: '';
    content: none;
  }

  b, strong {
    font-weight: bold;
  }

  i, em {
    font-style: italic;
  }

  strong, sup, sub, em, s, u {
    color: inherit;
  }

</style>

